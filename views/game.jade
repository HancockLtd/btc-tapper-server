extends layout

block content
  script.
    var socket = io.connect('http://localhost');

    socket.on('connected', function (data) {
      socket.emit('auth_user', { id: my_id });
    });

    //event handler for notification
    socket.on('notification', function (data) {
      $('.notification').text(data.text);
    });

